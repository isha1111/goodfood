<form action="/dishes" method="post">
  <label>Name </label>
  <input type="text" name = "name"></br>

  <label>Image url </label>
  <input type="text" name="image_url"></br>

  <label> Dish Type ID </label>
  <select name = "dish_id">
  <% @dish_types.each do |dish_type| %>
    <option value="<%= dish_type.id %>"> <%= dish_type.name %> </option>
  <% end %>
  </select>

  <button> Add Dish</button>
</form>
